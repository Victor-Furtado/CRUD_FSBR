@model CRUD_FSBR.Models.Processo

@{
    ViewData["Title"] = "CREATE";
}

<h1>CADASTRAR NOVO PROCESSO</h1>

<h4>Processo</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Nome" class="control-label"></label>
                <input asp-for="Nome" class="form-control" />
                <span asp-validation-for="Nome" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Npu" class="control-label"></label>
                <input asp-for="Npu" class="form-control" />
                <span asp-validation-for="Npu" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CadastradoEm" class="control-label"></label>
                <input asp-for="CadastradoEm" class="form-control" />
                <span asp-validation-for="CadastradoEm" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="VisualizadoEm" class="control-label"></label>
                <input asp-for="VisualizadoEm" class="form-control" />
                <span asp-validation-for="VisualizadoEm" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="UF" class="control-label"></label>
                <input asp-for="UF" class="form-control" />
                <span asp-validation-for="UF" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Municipio" class="control-label"></label>
                <input asp-for="Municipio" class="form-control" />
                <span asp-validation-for="Municipio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn btn-info">VOLTAR A LISTA</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
